			<key>Path</key>
			<string>/Applications/Safari.app/Contents/MacOS/Safari</string>
			<key>TitleKey</key>
			<string>Browse The Web</string>
		</dict>
		<dict>
			<key>BundlePath</key>
			<string>/Applications/Utilities/Disk Utility.app</string>
