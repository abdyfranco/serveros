				<key>Selector</key>
				<string>shouldShowRAIDUtility</string>
			</dict>
			<key>TitleKey</key>
			<string>RAID Utility</string>
		</dict>
		<dict>
			<key>BundlePath</key>
			<string>/Applications/Utilities/Terminal.app</string>
