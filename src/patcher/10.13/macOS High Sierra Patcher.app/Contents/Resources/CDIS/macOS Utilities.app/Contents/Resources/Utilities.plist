			<string>You have a backup of your system that you want to restore.</string>
			<key>Path</key>
			<string>/System/Installation/CDIS/Time Machine System Restore.app/Contents/MacOS/Time Machine System Restore</string>
			<key>TitleKey</key>
			<string>Restore From Time Machine Backup</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<array>
				<string>-RunInOSIEnv</string>
			</array>
			<key>BundlePath</key>
			<string>/Install macOS Sierra.app</string>
			<key>DescriptionKey</key>
			<string>Reinstall a new copy of macOS.</string>
			<key>IsInstallAssistant</key>
			<true/>
			<key>Path</key>
			<string>/Install macOS Sierra.app/Contents/MacOS/InstallAssistant</string>
			<key>TitleKey</key>
			<string>Reinstall macOS</string>
		</dict>
		<dict>
			<key>BundlePath</key>
			<string>/Applications/Safari.app</string>
